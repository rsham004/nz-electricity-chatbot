# AWS Credentials for Bedrock Access
# Replace with your actual AWS credentials after creating them
AWS_ACCESS_KEY_ID=your_access_key_id_here
AWS_SECRET_ACCESS_KEY=your_secret_access_key_here
AWS_DEFAULT_REGION=us-east-1

# Optional: Specific region for Bedrock (if different from default)
BEDROCK_REGION=us-east-1

# Optional: Model configuration
BEDROCK_MODEL_ID=anthropic.claude-3-5-sonnet-20241022-v2:0

# Claude API Key (alternative to AWS Bedrock)
ANTHROPIC_API_KEY=your_claude_api_key_here

# Electricity API Keys (if required)
EM6_API_KEY=your_em6_api_key_here
EMI_API_KEY=your_emi_api_key_here

# Optional: API endpoints
EM6_API_URL=https://api.em6.co.nz
EMI_API_URL=https://emi.developer.azure-api.net